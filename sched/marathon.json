{
  "id": "/pymesos-mesos",
  "instances": 1,
  "cpus": 0.1,
  "mem": 128,
  "disk": 0,
  "gpus": 0,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "taskKillGracePeriodSeconds": 30,
  "container": {
    "type": "MESOS",
    "docker": {
      "image": "luismorales/pymesos:6.0",
      "forcePullImage": true
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "portDefinitions": [
    {
      "port": 10103,
      "protocol": "tcp",
      "name": "default"
    }
  ],
  "requirePorts": true
}